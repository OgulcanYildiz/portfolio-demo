generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}




model Project {
  id          String   @id @default(cuid())
  slug        String   @unique
  title       String
  category    String
  year        String
  thumbnail   String
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
