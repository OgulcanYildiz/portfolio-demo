generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Project {
  id          String   @id @default(cuid())
  slug        String   @unique
  title       String
  category    String
  year        String
  thumbnail   String
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
